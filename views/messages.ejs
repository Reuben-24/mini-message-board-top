<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Messages</title>
</head>
<body>
  <h1>Messages</h2>
  <%- include('nav') %>
  <ul>
    <% if (messages) { %>
      <% messages.forEach(message => { %>
        <li>
          <div>ID: <%= message.id %> </div>
          <div>Message: <%= message.message %> </div>
          <div>Created At: <%= message.created_at %> </div>
        </li>
      <% }) %>
    <% } %>
  </ul>
</body>
</html>